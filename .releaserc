{
  "branches": [
    "release",
    {
      "name": "main",
      "prerelease": "rc"
    },
    {
      "name": "pre",
      "prerelease": "beta"
    }
  ],
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    "@semantic-release/github",
    [
      "@semantic-release/exec",
      {
        "prepareCmd": "./scripts/semantic-release/prepareCmd.sh v${nextRelease.version}",
        "publishCmd": "./scripts/semantic-release/publishCmd.sh v${nextRelease.version}"
      }
    ]
  ]
}